{
    "name": "Yuca/SingletonContainer",
    "type": "library",
    "description": "PSR-11 container decorator caching resolved instances.",
    "keywords": [
        "Yuca",
        "dependency injection",
        "DI",
        "PSR-11",
        "decorator",
        "singleton"
    ],
    "homepage": "https://github.com/Yuca/SingletonContainer",
    "license": "MIT",
    "authors": [
        {
            "name": "Hrvoje Jukic",
            "email": "hrcajuka@gmail.com",
            "homepage": "https://github.com/Yuca",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yuca\\SingletonContainer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuca\\SingletonContainer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
